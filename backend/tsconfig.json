{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist"
  },
  "include": [
    "./*.ts",
    "models/**/*.ts",
    "./**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
